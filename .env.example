# AI Models API Keys (Required)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MAPS_KEY=your_gemini_maps_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud
GOOGLE_CLOUD_PROJECT_ID=your_project_id

# LangSmith (Optional - for tracing)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key_here

# Other AI Services (Optional)
HUGGINGFACEHUB_API_TOKEN=your_huggingface_token
SERPAPI_API_KEY=your_serpapi_key
UPSTAGE_API_KEY=your_upstage_key
LLAMA_CLOUD_API_KEY=your_llama_cloud_key
PINECONE_API_KEY=your_pinecone_key
COHERE_API_KEY=your_cohere_key
TAVILY_API_KEY=your_tavily_key
GCP_API_KEY=your_gcp_api_key

# Naver API (Required for review analysis)
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret
NAVER_TREND_CLIENT_ID=your_naver_trend_client_id
NAVER_TREND_CLIENT_SECRET=your_naver_trend_client_secret

# Tour API
TOUR_API_KTY=your_tour_api_key

# Social Media (Optional)
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret
SEOUL_TOUR_API_KEY=your_seoul_tour_api_key

# Database Path (Optional - auto-detects sibling directory by default)
# Only set this if your tour_agent_database is in a non-standard location
# DATABASE_PATH=/path/to/tour_agent_database
